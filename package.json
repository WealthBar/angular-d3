{
  "name": "angularD3",
  "version": "0.0.35",
  "description": "AngularJS chart directives and other integrations for D3.js",
  "authors": [
    "Chris Nicola <chnicola@gmail.com>"
  ],
  "homepage": "http://wealthbar.github.io/angular-d3",
  "repository": {
    "type": "git",
    "url": "https://github.com/wealthbar/angular-d3.git"
  },
  "bugs": {
    "url": "https://github.com/wealthbar/angular-d3/issues"
  },
  "license": "MIT",
  "main": "dist/angularD3.js",
  "scripts": {
    "clean": "rimraf node_modules doc typings && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "e2e": "protractor",
    "test": "karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm run server",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "d3": "^3.5.12"
  },
  "devDependencies": {
    "bootstrap-loader": "^1.0.1",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "typings": "^0.3.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  }
}
